<table class="table table-striped">
  <tr>
    <th>Title</th>
    <th>No. of seates</th>
    <th>Cost per day</th>
    <th>Registration no.</th>
    <th>Milage</th> <th></th> <th></th>
  </tr>
  <% @available_vehicles.each do |vehicle|%>
    <tr>
      <td><%= vehicle.title %></td>
      <td><%= vehicle.no_of_seates %></td>
      <td><%= vehicle.cost_per_day %></td>
      <td><%= vehicle.registration_no %></td>
      <td><%= vehicle.milage %></td>
      <td><%= link_to 'Booking', new_booking_path(:car_id => vehicle.id) , :class => 'btn btn-mini'%></td>
    </tr>
  <% end %>
</table>


